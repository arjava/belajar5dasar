<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.arjava.limadasaran">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icons"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SplashScreen"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Alphabet"
            android:label="@string/title_activity_alphabet"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.arjava.limadasaran.MainActivity" />
        </activity>
        <activity
            android:name=".Angka"
            android:label="@string/title_activity_angka"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.arjava.limadasaran.MainActivity" />
        </activity>
        <activity
            android:name=".Buah"
            android:label="@string/title_activity_buah"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.arjava.limadasaran.MainActivity" />
        </activity>
        <activity
            android:name=".Warna"
            android:label="@string/title_activity_warna"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.arjava.limadasaran.MainActivity" />
        </activity>
        <activity
            android:name=".Hijaiyah"
            android:label="@string/title_activity_hijaiyah"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.arjava.limadasaran.MainActivity" />
        </activity>
        <activity android:name=".DetailAngka"
            android:screenOrientation="portrait"/>
        <activity android:name=".DetailAlphabet"
            android:screenOrientation="portrait"/>
        <activity android:name=".DetailWarna"
            android:screenOrientation="portrait"/>
        <activity android:name=".DetailBuah"
            android:screenOrientation="portrait"/>
        <activity android:name=".DetailHijaiyah"
            android:screenOrientation="portrait"/>
        <activity android:name=".About"
            android:screenOrientation="portrait"/>
        <activity android:name=".Pemesanan" />
        <activity android:name=".Kontak" />
        <activity android:name=".QuizTebakBuah"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            android:noHistory="true"></activity>
    </application>

</manifest>